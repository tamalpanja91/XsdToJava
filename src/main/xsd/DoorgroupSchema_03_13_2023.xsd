<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	attributeFormDefault="unqualified" elementFormDefault="qualified"
	targetNamespace="http://dotservice.doorgroup.com/Types"
	xmlns:tns="http://dotservice.doorgroup.com/Types">
	<!-- edited with his own tools (http://www.microsoft.com) by Marty Goepel 
		(ASSA ABLOY) -->
	<xs:element name="Order">
		<xs:annotation>
			<xs:documentation>Order information (root element)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="1" minOccurs="1"
					name="HeaderInformation">
					<xs:annotation>
						<xs:documentation>Order Header Information</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="1"
								name="Manufacturer" type="xs:string">
								<xs:annotation>
									<xs:documentation>Ceco, Curries, Fleming, etc.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="0" name="OrderGeneratedFully"
								type="xs:string" />
							<xs:element minOccurs="0" name="OrderCompletionInEDGE"
								type="xs:string" />
							<xs:element minOccurs="0" name="PriceBook"
								type="xs:string" />
							<xs:element minOccurs="0" name="OrderType"
								type="xs:string" >
								<xs:annotation>
									<xs:documentation>Order type D=Doors, F=Frames, B=Both
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="0" name="MfgLocation"
								type="xs:string">
								<xs:annotation>
									<xs:documentation>Manufacturer Use Only: Used to specify
										Service Center or Service Center orders</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="0" name="SCBranch"
								type="xs:string">
								<xs:annotation>
									<xs:documentation>Service Center Branch ID for AASC customer
										orders</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="1"
								name="MeasurementSystem">
								<xs:annotation>
									<xs:documentation>If none specified, Default = Imperial
									</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Imperial" />
										<xs:enumeration value="Metric" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element minOccurs="0" name="EDGEProjectID" type="xs:string" />
							<xs:element name="CustomerNumber" type="xs:string" />
							<xs:element minOccurs="0" name="POnumber"
								type="xs:string" />
							<xs:element minOccurs="0" name="CustPOnumber"
								type="xs:string" />
							<xs:element minOccurs="0" name="BrandQuoteNum"
								type="xs:string" />
							<xs:element form="qualified" minOccurs="0"
								name="QuoteNumber">
								<xs:complexType>
									<xs:sequence>
										<xs:element minOccurs="0" name="QuotePrefix"
											type="xs:string" />
										<xs:element minOccurs="0" name="QuoteNum"
											type="xs:integer" />
										<xs:element minOccurs="0" name="QuoteRevision"
											type="xs:integer" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element minOccurs="0" name="ProjectInfo">
								<xs:complexType>
									<xs:sequence>
										<xs:element minOccurs="0" name="MasterProjID"
											type="xs:string" />
										<xs:element minOccurs="0" name="MasterProjName"
											type="xs:string" />
										<xs:element minOccurs="0" name="ProjectName"
											type="xs:string" />
										<xs:element minOccurs="0" name="ProjectDesc"
											type="xs:string" />
										<xs:element minOccurs="0" name="ProjectAddress"
											type="xs:string" />
										<xs:element minOccurs="0" name="ProjectCity"
											type="xs:string" />
										<xs:element minOccurs="0" name="ProjectState"
											type="xs:string" />
										<xs:element minOccurs="0" name="ProjectZip"
											type="xs:string" />
										<xs:element minOccurs="0" name="ProjectCountry"
											type="xs:string" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="1"
								name="ProjectTransferSequenceNumber" type="xs:positiveInteger" />
							<xs:element minOccurs="0" name="RequiredDate"
								type="xs:date" />
							<xs:element minOccurs="0" name="EstimatedDate"
								type="xs:date" />
							<xs:element minOccurs="0" name="SalesInfo">
								<xs:complexType>
									<xs:sequence>
										<xs:element minOccurs="0" name="CustomerPo"
											type="xs:string" />
										<xs:element minOccurs="0" name="ProgramType"
											type="xs:string" />
										<xs:element minOccurs="0" name="CustomerName"
											type="xs:string" />
										<xs:element minOccurs="0" name="CustomerNumber"
											type="xs:string" />
										<xs:element minOccurs="0" name="QuoteNumber"
											type="xs:string" />
										<xs:element minOccurs="0" name="FrameUnits"
											type="xs:string" />
										<xs:element minOccurs="0" name="FrameLines"
											type="xs:string" />
										<xs:element minOccurs="0" name="DoorUnits"
											type="xs:string" />
										<xs:element minOccurs="0" name="DoorLines"
											type="xs:string" />
										<xs:element minOccurs="0" name="StockUnits"
											type="xs:string" />
										<xs:element minOccurs="0" name="StockLines"
											type="xs:string" />

									</xs:sequence>
								</xs:complexType>
							</xs:element>


							<xs:element minOccurs="0" name="SoldToInfo">
								<xs:complexType>
									<xs:sequence>
										<xs:element minOccurs="0" name="SoldToName"
											type="xs:string" />
										<xs:element minOccurs="0" name="SoldToAddress1"
											type="xs:string" />
										<xs:element minOccurs="0" name="SoldToCity"
											type="xs:string" />
										<xs:element minOccurs="0" name="SoldToState"
											type="xs:string" />
										<xs:element minOccurs="0" name="SoldToZip"
											type="xs:string" />
										<xs:element minOccurs="0" name="SoldToCountry"
											type="xs:string" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element maxOccurs="2" minOccurs="0"
								name="BillToInfo">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="BillToName" type="xs:string" />
										<xs:element minOccurs="0" name="BillToPhone"
											type="xs:string" />
										<xs:element minOccurs="0" name="BillToEmail"
											type="xs:string" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element minOccurs="0" name="ShippingInfo">
								<xs:complexType>
									<xs:sequence>
										<xs:element minOccurs="0" name="ShippingName"
											type="xs:string" />
										<xs:element minOccurs="0" name="ShippingAddress1"
											type="xs:string" />
										<xs:element minOccurs="0" name="ShippingAddress2"
											type="xs:string" />
										<xs:element minOccurs="0" name="ShippingAddress3"
											type="xs:string" />
										<xs:element minOccurs="0" name="ShippingCity"
											type="xs:string" />
										<xs:element minOccurs="0"
											name="ShippingProvinceState" type="xs:string" />
										<xs:element minOccurs="0" name="ShippingZip"
											type="xs:string" />
										<xs:element minOccurs="0" name="ShippingCountry"
											type="xs:string" />
										<xs:element minOccurs="0" name="ShipToJobsite"
											type="xs:string" />
										<xs:element minOccurs="0" name="AttentionTo"
											type="xs:string" />
										<xs:element minOccurs="0" name="ShipToNotice"
											type="xs:string" />
										<xs:element maxOccurs="unbounded" minOccurs="0"
											name="SHContactInfo">
											<xs:complexType>
												<xs:sequence>
													<xs:element minOccurs="0" name="SHContactName"
														type="xs:string" />
													<xs:element minOccurs="0"
														name="SHContactPhoneNumber" type="xs:string" />
													<xs:element minOccurs="0" name="SHContactEmail"
														type="xs:string" />
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element minOccurs="0" name="ShipVia"
											type="xs:string" />
										<xs:element minOccurs="0" name="ShipViaCode"
											type="xs:string" />
										<xs:element minOccurs="0" name="Carrier"
											type="xs:string" />
										<xs:element minOccurs="0" name="PrepaidCollect"
											type="xs:string" />
										<xs:element minOccurs="0"
											name="ShippingInstruction" type="xs:string" />
										<xs:element minOccurs="0" name="TransferShipment">
											<xs:complexType>
												<xs:sequence>
													<xs:element minOccurs="0" name="TSType">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:length value="1" />
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element minOccurs="0" name="TransferPoint">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="7" />
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element minOccurs="0"
														name="TransferPointDesc">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="60" />
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element minOccurs="0" name="TSShipVia">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="30" />
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element minOccurs="0" name="TSCarrier">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="5" />
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="ContactInfo">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ContactName" type="xs:string" />
										<xs:element minOccurs="0" name="ContactPhone"
											type="xs:string" />
										<xs:element minOccurs="0" name="ContactEmail"
											type="xs:string" />
										<xs:element minOccurs="0" name="ContactFax"
											type="xs:string" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element minOccurs="0" name="BuyOutInfo">
								<xs:complexType>
									<xs:sequence>
										<xs:element minOccurs="0" name="BuyOutOrderNo"
											type="xs:string" />
										<xs:element minOccurs="0" name="BuyOutLocation"
											type="xs:string" />
										<xs:element minOccurs="0" name="BuyOutHeaderType"
											type="xs:string" />
											<xs:element minOccurs="0" name="EDGEProjectID"
											type="xs:string" />
											</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element minOccurs="0" maxOccurs="unbounded" name="Notifications">
								<xs:complexType>
									<xs:sequence>
										<xs:element minOccurs="0" name="EmailAddress"
											type="xs:string" />
										<xs:element minOccurs="0" name="Category"
											type="xs:string" />
											</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="AttachmentInfo">
								<xs:annotation>
									<xs:documentation>PDF Order Cover Sheet Recommended
									</xs:documentation>
									<xs:documentation>PDF of Original Order Required
									</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="AttachmentID" type="xs:string">
											<xs:annotation>
												<xs:documentation>Attachment File Name and extension
												</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element minOccurs="0" name="AttachmentRef"
											type="xs:string">
											<xs:annotation>
												<xs:documentation>Fax Reference Name</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element minOccurs="0"
											name="NumberofAttachments" type="xs:positiveInteger">
											<xs:annotation>
												<xs:documentation>Number of Faxed Attachments
												</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element minOccurs="0" name="AttachmentFile"
											type="xs:base64Binary">
											<xs:annotation>
												<xs:documentation>Base 64 Encoded attachment (PDF, Word,
													Excel, JPG, GIF, TIF)</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element minOccurs="0" name="RefOrderNo"
								type="xs:string">
								<xs:annotation>
									<xs:documentation>Manufacturer Use Only</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="0" name="OrderNo"
								type="xs:string">
								<xs:annotation>
									<xs:documentation>Manufacturer Use Only</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="0" name="JdeOrderNo"
								type="xs:string">
								<xs:annotation>
									<xs:documentation>JDE order number when used with AASC
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ProgramType" type="xs:string" />
							<xs:element minOccurs="0" name="ProgramTypeDesc"
								type="xs:string" />
							<xs:element minOccurs="0" name="ProgramCategory"
								type="xs:string" />
							<xs:element minOccurs="0" name="GeneralNotes"
								type="xs:string">
								<xs:annotation>
									<xs:documentation>General Notes should be included on Order
										Cover Page and/or Order Form</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="0" name="TotalDoorQty"
								type="xs:positiveInteger" />
							<xs:element minOccurs="0" name="TotalFrameQty"
								type="xs:positiveInteger" />
							<xs:element minOccurs="0" name="PreTaxTotal">
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="9" />
										<xs:fractionDigits value="2" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element minOccurs="0" name="HeaderTotalPrice">
								<xs:annotation>
									<xs:documentation>Manufacturer Use Only</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="9" />
										<xs:fractionDigits value="2" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element minOccurs="0" name="HeaderDiscount">
								<xs:annotation>
									<xs:documentation>Manufacturer Use Only</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="9" />
										<xs:fractionDigits value="4" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element minOccurs="0" name="SalesTax">
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="7" />
										<xs:fractionDigits value="2" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element minOccurs="0" name="PrintRoute"
								type="xs:string" />
							<xs:element minOccurs="0" name="Action"
								type="xs:string" />
							<xs:element minOccurs="0" name="SenderName"
								type="xs:string" />
							<xs:element minOccurs="0" name="SenderUserName"
								type="xs:string" />
							<xs:element minOccurs="0" name="SenderEmail"
								type="xs:string" />
							<xs:element minOccurs="0" name="SenderPhone"
								type="xs:string" />
							<xs:element minOccurs="0" name="Category"
								type="xs:string" />
							<xs:element minOccurs="0" name="RemakeCode"
								type="xs:string" />
							<xs:element minOccurs="0" name="OriginalOrder"
								type="xs:string" />
							<xs:element minOccurs="0" name="OriginalShipYear"
								type="xs:string" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element maxOccurs="unbounded" name="OrderLine">
					<xs:complexType>
						<xs:sequence>
							<xs:element minOccurs="0" name="Brand"
								type="xs:string" />
							<xs:element minOccurs="0" name="LineGeneratedFully"
								type="xs:string" />
							<xs:element minOccurs="0" name="Plant-Whse"
								type="xs:string" />
							<xs:element minOccurs="0" name="ShipDate"
								type="xs:string" />
							<xs:element name="ProductType" type="xs:string" />
							<xs:element name="LineNumber"
								type="xs:positiveInteger" />
							<xs:element minOccurs="0" name="JDELineNumber"
								type="xs:string" />
							<xs:element minOccurs="0" name="POLineNumber"
								type="xs:string" />
							<xs:element minOccurs="0" name="PartNumber"
								type="xs:string" />
							<xs:element minOccurs="0" name="CusPartNumber"
								type="xs:string" />
							<xs:element minOccurs="0" name="PartDescription"
								type="xs:string" />
							<xs:element name="Quantity" type="xs:positiveInteger" />
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="MarkNumber">
								<xs:complexType>
									<xs:sequence>
										<xs:element minOccurs="0" name="MarkID"
											type="xs:string" />
										<xs:element minOccurs="0" name="Qty"
											type="xs:nonNegativeInteger" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element minOccurs="0" name="Weight"
								type="xs:positiveInteger">
								<xs:annotation>
									<xs:documentation>Manufacturer use only</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element minOccurs="0" name="UnitPrice">
								<xs:annotation>
									<xs:documentation>Manufacturer use only</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="7" />
										<xs:fractionDigits value="2" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element minOccurs="0" name="UnitNetPrice">
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="7" />
										<xs:fractionDigits value="2" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element minOccurs="0" name="LinePrice">
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="7" />
										<xs:fractionDigits value="2" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element minOccurs="0" name="LineDiscount">
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="7" />
										<xs:fractionDigits value="2" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>

							<xs:element minOccurs="0" name="Drawings">
								<xs:complexType>
									<xs:sequence>
										<xs:element minOccurs="0" name="DrawingVersion">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="10" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element maxOccurs="unbounded" minOccurs="0"
											name="DrawingElevation">
											<xs:complexType>
												<xs:sequence>
													<xs:element minOccurs="0" name="ElevName">
														<xs:annotation>
															<xs:documentation>Elevation Name and file extension
															</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="10" />
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element minOccurs="0" name="ElevData"
														type="xs:base64Binary">
														<xs:annotation>
															<xs:documentation>PRO-TECH/Manufacturer Use Only
															</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element minOccurs="0" name="ElevImage"
														type="xs:base64Binary">
														<xs:annotation>
															<xs:documentation>Base 64 Encoded attachment (PDF, Word,
																Excel, JPG, GIF, TIF)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element maxOccurs="unbounded" minOccurs="0"
														name="ElevCrossSection">
														<xs:complexType>
															<xs:sequence>
																<xs:element minOccurs="0" name="XsecName">
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:maxLength value="10" />
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element minOccurs="0" name="XsecData"
																	type="xs:base64Binary">
																	<xs:annotation>
																		<xs:documentation>PRO-TECH/Manufacturer Use Only
																		</xs:documentation>
																	</xs:annotation>
